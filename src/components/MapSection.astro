---
const mapEmbedUrl =
    'https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3303.123456789!2d-115.17281958478292!3d36.17083417949344!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x80c8c123456789%3A0xabcdef123456789!2s123%20S%204th%20St%2C%20Las%20Vegas%2C%20NV%2089101!5e0!3m2!1ses!2sus!4v1699999999999!5m2!1ses!2sus';
---

<section>
    <div>
        <div class="h-96 w-full">
            <iframe
                src={mapEmbedUrl}
                width="100%"
                height="100%"
                style="border:0;"
                allowfullscreen=""
                loading="lazy"
                referrerpolicy="no-referrer-when-downgrade"></iframe>
        </div>
    </div>
</section>
